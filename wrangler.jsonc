{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "cf_ai_vector_database",
  "main": "src/index.ts",
  "compatibility_date": "2025-10-26",
  "observability": {
    "enabled": true
  },

  "assets": {
    "directory": "./public/"
  },

  "kv_namespaces": [
    {
      "binding": "VECTOR_KV",
      "id": "469f34a97b5c4718bb9f80c0f37804c4"  // Get this from Cloudflare dashboard
    }
  ],
  "ai": {
    "binding": "AI"
  }
}
